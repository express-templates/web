{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  {{#ts}}
  "main": "./dist/app.js",
  {{/ts}}
  {{#if_eq ts false}}
  "main": "./src/app.js",
  "scripts": {
    {{#ts}}
    "start": "etsc && node ./dist/index.js",
    "dev": "nodemon app.ts",
    {{/ts}}
    {{#if_eq ts false}}
    "start": "node app.js",
    "dev": "nodemon app.js",
    {{/if_eq}}
    {{#ts}}
    "build": "etsc",
    {{/ts}}
    "lint": "eslint --fix",
    "test": "mocha --exit"
  },
  "author": "{{ author }}",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "helmet": "^4.4.1",
    "morgan": "^1.10.0",
    "module-alias": "^2.2.2",
    {{#if_eq database "mongoose"}}
    "mongoose": "^5.12.5",
    {{/if_eq}}
    {{#if_eq database "mysql"}}
    "mysql": "^2.18.1",
    {{/if_eq}}
    {{#if_xor axios extraction "extraction"}}
    "axios": "^0.21.1",
    "axios-cache-adapter": "^2.7.3",
    {{/if_xor}}
    {{#if_xor jsdom extraction "extraction"}}
    "jsdom": "^16.5.3",
    {{/if_xor}}
    "cookie-parser": "^1.4.5",
    {{#if_eq view "jade"}}
    "jade": "^1.11.0",
    {{/if_eq}}
    {{#if_eq view "ejs"}}
    "ejs": "^3.1.6",
    {{/if_eq}}
    {{#if_eq view "pug"}}
    "pug": "^3.0.2",
    {{/if_eq}}
    {{#if_eq view "hbs"}}
    "hbs": "^4.1.2",
    {{/if_eq}}
    {{#if_eq view "hjs"}}
    "hjs": "^0.0.6",
    "adaro": "^1.0.4",
    {{/if_eq}}
    {{#if_eq view "dust"}}
    "dust": "^0.3.0",
    {{/if_eq}}
    {{#if_eq view "twig"}}
    "twig": "^1.15.4",
    {{/if_eq}}
    {{#if_eq view "vash"}}
    "vash": "^0.13.0",
    {{/if_eq}}
    {{#if_eq stylesheet "less"}}
    "less-middleware": "^3.1.0",
    {{/if_eq}}
    {{#if_eq stylesheet "stylus"}}
    "stylus": "^0.54.8",
    {{/if_eq}}
    {{#if_eq stylesheet "compass"}}
    "node-compass": "^0.2.4",
    {{/if_eq}}
    {{#if_in stylesheet "sass" "scss"}}
    "node-sass-middleware": "^0.11.0",
    {{/if_in}}
    "express-import-routes": "^0.0.4-b1"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^9.0.0",
    "mocha": "^9.1.3",
    "supertest": "^6.1.6",
    {{#ts}}
    "esbuild-node-tsc": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    {{#if_xor jsdom extraction "extraction"}}
    "@types/jsdom": "^16.2.10",
    {{/if_xor}}
    {{#if_xor axios extraction "extraction"}}
    "@types/axios": "^0.14.0",
    {{/if_xor}}
    {{#if_eq database "mysql"}}
    "@types/mysql": "^2.15.18",
    {{/if_eq}}
    {{#if_eq database "mongoose"}}
    "@types/mongoose": "^5.10.5",
    {{/if_eq}}
    "@types/morgan": "^1.9.2",
    "@types/helmet": "^4.0.0",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^15.0.1",
    "@types/mocha": "^9.0.0",
    "@types/module-alias": "^2.0.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    {{#if_eq view "jade"}}
    "@types/jade": "^0.0.30",
    {{/if_eq}}
    {{#if_eq view "ejs"}}
    "@types/ejs": "^3.0.6",
    {{/if_eq}}
    {{#if_eq view "pug"}}
    "@types/pug": "^2.0.4",
    {{/if_eq}}
    {{#if_eq view "hbs"}}
    "@types/hbs": "^4.0.1",
    {{/if_eq}}
    {{#if_eq view "twig"}}
    "@types/twig": "^1.12.5",
    {{/if_eq}}
    {{#if_eq stylesheet "less"}}
    "@types/less-middleware": "^2.0.31",
    {{/if_eq}}
    {{#if_eq stylesheet "stylus"}}
    "@types/stylus": "^0.48.34",
    {{/if_eq}}
    {{/ts}}
    "nodemon": "^2.0.7"
  }
}
